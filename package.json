  "repository": {
    "type": "git",
    "url": "git+https://github.com/mapbox/scroll-restorer.git"
  },
  "keywords": [
    "scroll-restoration",
    "scroll",
    "restore",
    "routing",
    "router"
  ],
  "author": "Mapbox",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mapbox/scroll-restorer/issues"
  },
  "homepage": "https://github.com/mapbox/scroll-restorer#readme",
  "devDependencies": {
    "babel": "^6.23.0",
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "eslint": "^4.1.1",
    "eslint-plugin-react": "^7.1.0",
    "husky": "^0.14.1",
    "lint-staged": "^4.0.0",
    "prettier": "^1.5.2",
    "prop-types": "^15.5.10",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-router-dom": "^4.1.1",
    "webpack": "^3.0.0",
    "webpack-dev-server": "^2.5.0"
  },
  "lint-staged": {
    "{,test/**/}*.js": [
